    mt_sft: mt_sft {
         compatible = "zmk,behavior-hold-tap";
         label = "MT_SHIFT";
         #binding-cells = <2>;
         flavor = "hold-preferred";
         tapping-term-ms = <1024>;
         quick-tap-ms = <128>;
         bindings = <&sl>, <&to>;
    };
    
    td_qu: qu_tap_dance {
        compatible = "zmk,behavior-tap-dance";
        label = "td_qu";
        #binding-cells = <0>;
        tapping-term-ms = <200>;
        bindings = <&kp U>, <&qu>;
    };

    td_qu_cap: qu_cap_tap_dance {
        compatible = "zmk,behavior-tap-dance";
        label = "td_qu_cap";
        #binding-cells = <0>;
        tapping-term-ms = <200>;
        bindings = <&kp LS(U)>, <&qu_cap>;
    };

    td_copy_paste: td_copy_paste {
        compatible = "zmk,behavior-tap-dance";
        label = "td_copy_paste";
        #binding-cells = <0>;
        tapping-term-ms = <300>;
        //bindings = <&ctrl_v>, <&ctrl_c>;
        bindings = <&kp k_paste>, <&kp k_copy>;
    };


